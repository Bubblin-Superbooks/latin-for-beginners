  {
    "tag": "h4",
    "innerHtml": "M. Lēgātus Nōn Est, Sed Est Apud Legiōnāriōs."
  },
  {
    "tag": "hr",
    "innerHtml": ""
  },
  {
    "tag": "h4",
    "innerHtml": "C. Quae5 Arma Portat6?"
  },
  {
    "tag": "hr",
    "innerHtml": ""
  },
  {
    "tag": "h4",
    "innerHtml": "M. Scūtum Magnum Et Lōrīcam Dūram Et Galeam Pulchram Portat."
  },
  {
    "tag": "hr",
    "innerHtml": ""
  },
  {
    "tag": "h4",
    "innerHtml": "C. Quae Tēla Portat?"
  },
  {
    "tag": "hr",
    "innerHtml": ""
  },
  {
    "tag": "h4",
    "innerHtml": "M. Gladium Et Pīlum Longum Portat."
  },
  {
    "tag": "hr",
    "innerHtml": ""
  },
  {
    "tag": "h4",
    "innerHtml": "C. Amatne Lēgātus Fīlium Tuum?"
  },
  {
    "tag": "hr",
    "innerHtml": ""
  },
  {
    "tag": "h4",
    "innerHtml": "M. Amat, Et Saepe Fīliō Meō Praemia Pulchra Et Praedam Multam Dat."
  },
